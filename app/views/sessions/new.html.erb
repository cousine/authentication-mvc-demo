<h1>Please login</h1>

<%= form_tag sessions_path do %>
  <%= label_tag :email %>
  <%= email_field_tag :email, @user.email, name: 'user[email]', id: 'email' %>

  <%= label_tag :password %>
  <%= password_field_tag :password, '', name: 'user[password]', id: 'password' %>

  <%= submit_tag 'Login' %>
<% end %>

